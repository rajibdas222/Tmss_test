{"ast":null,"code":"export default {\n  name: 'department-update-page',\n  data: function () {\n    return {\n      student: {\n        name: ''\n      },\n      errors: {\n        name: ''\n      }\n    };\n  },\n  watch: {\n    'student.name': function (newName) {\n      this.errors.name = newName ? '' : 'Name is required';\n    }\n  },\n  computed: {\n    isFormValid: function () {\n      let valid = true;\n      const self = this;\n\n      for (const [key, value] of Object.entries(self.errors)) {\n        if (self.student[key] == '' || value != '') {\n          valid = false;\n          break;\n        }\n      }\n\n      return valid;\n    }\n  },\n  mounted: function () {\n    this.show(this.$route.params.id);\n  },\n  methods: {\n    show: function (id) {\n      const self = this;\n      self.$store.commit('loading', true);\n      self.axios.get(self.$api_url + '/students/' + id + '/show', {\n        withCredentials: true\n      }).then(res => {\n        self.department = res.data;\n      }).catch(err => {\n        const message = err.response.data.message ? err.response.data.message : err.message;\n        self.$toast.error(message);\n        self.$router.push('/students');\n      }).finally(() => {\n        self.$store.commit('loading', false);\n      });\n    },\n    update: function (e, id) {\n      e.preventDefault();\n      const self = this;\n      if (self.isFormValid == false) return false;\n      self.$store.commit('loading', true);\n      self.axios.put(self.$api_url + '/students/' + id + '/update', self.post, {\n        withCredentials: true\n      }).then(res => {\n        console.log(res);\n        self.$toast.success(res.data.message);\n        self.$router.push('/students');\n      }).catch(err => {\n        const message = err.response.data.message ? err.response.data.message : err.message;\n        self.$toast.error(message);\n        self.$router.push('/students');\n      }).finally(() => {\n        self.$store.commit('loading', false);\n      });\n    }\n  }\n};","map":{"version":3,"mappings":"AA8CI,eAAe;EACXA,IAAI,EAAE,wBADK;EAEXC,IAAI,EAAE,YAAU;IACZ,OAAO;MACHC,OAAM,EAAI;QACNF,IAAG,EAAI;MADD,CADP;MAIHG,MAAK,EAAI;QACLH,IAAG,EAAI;MADF;IAJN,CAAP;EAQH,CAXU;EAYXI,KAAK,EAAC;IACF,gBAAgB,UAASC,OAAT,EAAiB;MAC7B,KAAKF,MAAL,CAAYH,IAAZ,GAAoBK,OAAM,GAAI,EAAJ,GAAS,kBAAnC;IACH;EAHC,CAZK;EAkBXC,QAAQ,EAAC;IACLC,WAAW,EAAE,YAAU;MACnB,IAAIC,KAAI,GAAI,IAAZ;MACA,MAAMC,IAAG,GAAI,IAAb;;MACA,KAAK,MAAM,CAACC,GAAD,EAAMC,KAAN,CAAX,IAA2BC,MAAM,CAACC,OAAP,CAAeJ,IAAI,CAACN,MAApB,CAA3B,EAAwD;QACpD,IAAGM,IAAI,CAACP,OAAL,CAAaQ,GAAb,KAAqB,EAArB,IAA2BC,KAAI,IAAK,EAAvC,EAA0C;UACtCH,KAAI,GAAI,KAAR;UACA;QACJ;MACJ;;MACA,OAAOA,KAAP;IACJ;EAXK,CAlBE;EA+BXM,OAAO,EAAE,YAAW;IAChB,KAAKC,IAAL,CAAU,KAAKC,MAAL,CAAYC,MAAZ,CAAmBC,EAA7B;EACH,CAjCU;EAkCXC,OAAM,EAAI;IAENJ,IAAI,EAAE,UAASG,EAAT,EAAY;MACd,MAAMT,IAAG,GAAI,IAAb;MACAA,IAAI,CAACW,MAAL,CAAYC,MAAZ,CAAmB,SAAnB,EAA8B,IAA9B;MACAZ,IAAI,CAACa,KAAL,CAAWC,GAAX,CAAed,IAAI,CAACe,QAAL,GAAgB,YAAhB,GAA6BN,EAA7B,GAAgC,OAA/C,EAAwD;QAAEO,eAAe,EAAE;MAAnB,CAAxD,EACKC,IADL,CACUC,GAAE,IAAK;QACTlB,IAAI,CAACmB,UAAL,GAAkBD,GAAG,CAAC1B,IAAtB;MACH,CAHL,EAIK4B,KAJL,CAIWC,GAAE,IAAK;QACV,MAAMC,OAAM,GAAID,GAAG,CAACE,QAAJ,CAAa/B,IAAb,CAAkB8B,OAAlB,GAA4BD,GAAG,CAACE,QAAJ,CAAa/B,IAAb,CAAkB8B,OAA9C,GAAwDD,GAAG,CAACC,OAA5E;QACAtB,IAAI,CAACwB,MAAL,CAAYC,KAAZ,CAAkBH,OAAlB;QACAtB,IAAI,CAAC0B,OAAL,CAAaC,IAAb,CAAkB,WAAlB;MACH,CARL,EASKC,OATL,CASa,MAAM;QACX5B,IAAI,CAACW,MAAL,CAAYC,MAAZ,CAAmB,SAAnB,EAA8B,KAA9B;MACH,CAXL;IAYH,CAjBK;IAmBNiB,MAAM,EAAE,UAASC,CAAT,EAAYrB,EAAZ,EAAe;MACnBqB,CAAC,CAACC,cAAF;MACA,MAAM/B,IAAG,GAAI,IAAb;MACA,IAAGA,IAAI,CAACF,WAAL,IAAoB,KAAvB,EACI,OAAO,KAAP;MAEJE,IAAI,CAACW,MAAL,CAAYC,MAAZ,CAAmB,SAAnB,EAA8B,IAA9B;MACAZ,IAAI,CAACa,KAAL,CAAWmB,GAAX,CAAehC,IAAI,CAACe,QAAL,GAAgB,YAAhB,GAA+BN,EAA/B,GAAoC,SAAnD,EAA8DT,IAAI,CAACiC,IAAnE,EAAyE;QAAEjB,eAAe,EAAE;MAAnB,CAAzE,EACKC,IADL,CACUC,GAAE,IAAK;QACTgB,OAAO,CAACC,GAAR,CAAYjB,GAAZ;QACAlB,IAAI,CAACwB,MAAL,CAAYY,OAAZ,CAAoBlB,GAAG,CAAC1B,IAAJ,CAAS8B,OAA7B;QACAtB,IAAI,CAAC