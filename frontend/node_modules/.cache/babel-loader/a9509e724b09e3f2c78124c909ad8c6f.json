{"ast":null,"code":"export default {\n  name: 'department-create-page',\n  data: function () {\n    return {\n      student: {\n        name: '',\n        batch_name: ''\n      },\n      errors: {\n        name: '',\n        batch_name: ''\n      }\n    };\n  },\n\n  mounted() {},\n\n  watch: {\n    'student.name': function (newName) {\n      this.errors.student = newName ? '' : 'Name is required';\n    }\n  },\n  computed: {\n    isFormValid: function () {\n      let valid = true;\n      const self = this;\n\n      for (const [key, value] of Object.entries(self.errors)) {\n        if (self.student[key] == '' || value != '') {\n          valid = false;\n          break;\n        }\n      }\n\n      return valid;\n    }\n  },\n  methods: {\n    clear: function () {\n      this.student = {\n        name: ''\n      };\n    },\n    create: function (e) {\n      e.preventDefault();\n      const self = this;\n      if (self.isFormValid == false) return false;\n      self.$store.commit('loading', true);\n      self.axios.post(self.$api_url + '/students', self.student, {\n        withCredentials: true\n      }).then(res => {\n        self.$toast.success(res.data.message);\n        self.clear();\n        self.$router.push('/students');\n      }).catch(err => {\n        const message = err.response.data.message ? err.response.data.message : err.message;\n        self.$toast.error(message);\n      }).finally(() => {\n        self.$store.commit('loading', false);\n      });\n    }\n  }\n};","map":{"version":3,"mappings":"AA6CI,eAAe;EACXA,IAAI,EAAE,wBADK;EAEXC,IAAI,EAAE,YAAU;IACZ,OAAO;MACHC,OAAM,EAAI;QACNF,IAAG,EAAI,EADD;QAENG,UAAS,EAAI;MAFP,CADP;MAKHC,MAAK,EAAI;QACLJ,IAAG,EAAI,EADF;QAELG,UAAS,EAAI;MAFR;IALN,CAAP;EAUH,CAbU;;EAeXE,OAAO,GAAG,CAET,CAjBU;;EAmBXC,KAAK,EAAC;IACF,gBAAgB,UAASC,OAAT,EAAiB;MAC7B,KAAKH,MAAL,CAAYF,OAAZ,GAAuBK,OAAM,GAAI,EAAJ,GAAS,kBAAtC;IACH;EAHC,CAnBK;EAyBXC,QAAQ,EAAC;IACLC,WAAW,EAAE,YAAU;MACnB,IAAIC,KAAI,GAAI,IAAZ;MACA,MAAMC,IAAG,GAAI,IAAb;;MACA,KAAK,MAAM,