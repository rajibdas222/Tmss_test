{"ast":null,"code":"export default {\n  name: 'admin-post-page',\n  data: function () {\n    return {\n      post: {\n        name: ''\n      },\n      errors: {\n        name: ''\n      }\n    };\n  },\n  watch: {\n    'post.name': function (newName) {\n      this.errors.title = newName ? '' : 'Name is required';\n    }\n  },\n  computed: {\n    isFormValid: function () {\n      let valid = true;\n      const self = this;\n\n      for (const [key, value] of Object.entries(self.errors)) {\n        if (self.post[key] == '' || value != '') {\n          valid = false;\n          break;\n        }\n      }\n\n      return valid;\n    }\n  },\n  methods: {\n    clear: function () {\n      this.post = {\n        title: '',\n        status: 'draft',\n        description: ''\n      };\n    },\n    create: function (e) {\n      e.preventDefault();\n      const self = this;\n      if (self.isFormValid == false) return false;\n      self.$store.commit('loading', true);\n      self.axios.post(self.$api_url + 'admin/posts', self.post, {\n        withCredentials: true\n      }).then(res => {\n        self.$toast.success(res.data.message);\n        self.clear();\n      }).catch(err => {\n        const message = err.response.data.message ? err.response.data.message : err.message;\n        self.$toast.error(message);\n      }).finally(() => {\n        self.$store.commit('loading', false);\n      });\n    }\n  }\n};","map":{"version":3,"mappings":"AA6BI,eAAe;EACXA,IAAI,EAAE,iBADK;EAEXC,IAAI,EAAE,YAAU;IACZ,OAAO;MACHC,IAAG,EAAI;QACHF,IAAG,EAAI;MADJ,CADJ;MAIHG,MAAK,EAAI;QACLH,IAAG,EAAI;MADF;IAJN,CAAP;EAQH,CAXU;EAYXI,KAAK,EAAC;IACF,aAAa,UAASC,OAAT,EAAiB;MAC1B,KAAKF,MAAL,CAAYG,KAAZ,GAAqBD,OAAM,GAAI,EAAJ,GAAS,kBAApC;IACH;EAHC,CAZK;EAkBXE,QAAQ,EAAC;IACLC,WAAW,EAAE,YAAU;MACnB,IAAIC,KAAI,GAAI,IAAZ;MACA,MAAMC,IAAG,GAAI,IAAb;;MACA,KAAK,MAAM,CAACC,GAAD,EAAMC,KAAN,CAAX,IAA2BC,MAAM,CAACC,OAAP,CAAeJ,IAAI,CAACP,MAApB,CAA3B,EAAwD;QACpD,IAAGO,IAAI,CAACR,IAAL,CAAUS,GAAV,KAAkB,EAAlB,IAAwBC,KAAI,IAAK,EAA